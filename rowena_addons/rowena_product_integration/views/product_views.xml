<odoo>
    <record id="product_supplierinfo_form_view_costumization" model="ir.ui.view">
        <field name="name">product.supplierinfo.form.view.costumization</field>
        <field name="model">product.supplierinfo</field>
        <field name="inherit_id" ref="product.product_supplierinfo_form_view"/>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='product_code']" position="after">
                <field name="is_api_create"></field>
            </xpath>
        </field>
    </record>
</odoo>